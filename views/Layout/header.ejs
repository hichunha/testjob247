<script>
    const sidebarEl = document.querySelector('.wrapper-sidebar')
    const btnMenu = document.querySelector('.more_cate')

    let showSidebar = true;
    btnMenu.addEventListener('click', function () {
        if (showSidebar === false) {
            sidebarEl.style.display = 'none';
            showSidebar = true;
        } else {
            sidebarEl.style.display = 'flex';
            showSidebar = false;
        }
    })

    window.addEventListener('click', function (event) {
        if (!event.target.closest('.wrapper-sidebar') && !event.target.closest('.more_cate')) {
            if (showSidebar === false) {
                sidebarEl.style.display = 'none';
                showSidebar = true;
            }
        }
    })
</script>