<body>
  <%- include('layouts/header', {}); %>
  <div class="candidate-profile">
    <span class="profile-section cv-manage">
      <span class="cv-manage-btn">
        <span class="onecvmanage-btn cvtemplate-btn">
          <span class="btn-name template-txt">CV template</span>
          <img src="../images/cvtemplate.png" alt="image" class="cvmanagebtn-image template-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
        </span>
        <span class="onecvmanage-btn changecolor-btn">
          <span class="btn-name color-txt">Change CV color</span>
          <span class="current-color-ic"></span>
        </span>
        <span class="onecvmanage-btn changelanguage-btn">
          <span class="btn-name language-txt">Change CV language</span>
          <img src="../images/cvlanguage.png" alt="image" class="cvmanagebtn-image language-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
        </span>
      </span>
      <span class="cv-act">
        <span class="onecvact-btn duplicate-btn">
          <img src="../images/duplicate.png" alt="icon" class="cvactbtn-icon duplicate-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
          <span class="actbtn-name duplicate-txt">Duplicate</span>
        </span>
        <span class="onecvact-btn preview-btn">
          <img src="../images/preview.png" alt="icon" class="cvactbtn-icon preview-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
          <span class="actbtn-name preview-txt">Preview</span>
        </span>
        <span class="onecvact-btn savecv-btn">
          <img src="../images/save.png" alt="icon" class="cvactbtn-icon savecv-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
          <span class="actbtn-name savecv-txt">Save CV</span>
        </span>
        <span class="onecvact-btn download-btn">
          <img src="../images/download.png" alt="icon" class="cvactbtn-icon download-ic"
            onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
          <span class="actbtn-name download-txt">Download</span>
        </span>
      </span>
    </span>
    <div class="profile-section file-infor">
      <div class="my-profile">
        <span class="oneprofile-section personalsummary-section">
          <div class="profilesection-icon-name">
            <img src="../images/personalsummary.png" alt="image" class="profilesection-icon personalsummary-ic"
              onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
            <span class="profilesection-name personalsummary-txt">Personal summary</span>
          </div>
          <div class="profilesection-detail-addthissection">
            <span class="profilesection-detail">Add a personal summary to your profile as a way to introduce who you
              are.</span>
            <span class="profilesection-addthissection-btn">Add summary</span>
          </div>
        </span>
        <span class="oneprofile-section careerhistory-section">
          <div class="profilesection-icon-name">
            <img src="../images/careerhistory.png" alt="image" class="profilesection-icon careerhistory-ic"
              onerror="this.onerror=null; this.src='/images/img/avata_default.png';">
            <span class="profilesection-name careerhistory-txt">Career history</span>
          </div>
          <div class="profilesection-detail-addthissection">
            <span class="profilesection-detail">The more you let employers know about your experience, the more you can
              stand out.</span>
            <span class="profilesection-addthissection-btn">Add role</span>
          </div>
        </span>
        <span class="oneprofile-section education-section">
          <div class="profilesection-icon-name">
            <img src="../images/education.png" alt="image" class="profilesection-icon education-ic"
