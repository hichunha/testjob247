                document.querySelector('.btn_FAQ').addEventListener('click', function () {
                    const modal = document.querySelector('.modal_FAQ');
                    modal.style.display = (modal.style.display === 'block') ? 'none' : 'block';
                });

                document.querySelectorAll('.btn_plusFAQ').forEach(button => {
                    button.addEventListener('click', function () {
                        const answer = this.closest('.question').querySelector('.answer');
                        const icon = this.querySelector('img');

                        // Toggle the display of the answer
                        answer.style.display = answer.style.display === 'block' ? 'none' : 'block';

                        // Toggle the plus/minus icon
                        if (icon.src.includes('plusFAQ.svg')) {
                            icon.src = '/images/icon/minusFAQ.svg';  // Change to minus
                        } else {
                            icon.src = '/images/icon/plusFAQ.svg';  // Change back to plus
                        }
                    });
                });
                function updateImageSrc() {
                    var img = document.getElementById("faqImage");
                    if (window.innerWidth <= 430) {
                        img.src = "/images/img/FAQ_430.png";
                    } else {
                        img.src = "/images/img/FAQ.png";
                    }
                }

                // Gọi hàm khi tải trang
                updateImageSrc();

                // Lắng nghe sự kiện resize để thay đổi khi kích thước màn hình thay đổi
                window.addEventListener("resize", updateImageSrc);
            </script>
